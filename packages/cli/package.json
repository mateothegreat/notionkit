{
  "name": "@mateothegreat/notion-sync",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "sideEffects": false,
  "scripts": {
    "dev": "npm-run-all2 --parallel build:watch dev:run:export",
    "dev:run:export": "find . -not -path '**/node_modules/*' | entr -c bin/dev.js export",
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc -p tsconfig.json --watch",
    "dev:dev": "vite",
    "prepack": "npm run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:coverage:watch": "vitest run --coverage --watch",
    "test:totals": "npm run test 2>/dev/null | grep -iE \"(test files|tests|passed|failed)\" | tail -3"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "main": "dist/index.js",
  "exports": {
    ".": "./src/index.ts"
  },
  "devDependencies": {
    "@notion.codes/types": "workspace:*",
    "@notion.codes/sdk": "workspace:*",
    "@notionhq/client": "4.0.1",
    "@oclif/core": "4.5.1",
    "@oclif/prettier-config": "0.2.1",
    "@oclif/test": "4.1.13",
    "@types/config": "^3.3.5",
    "@types/fast-levenshtein": "0.0.4",
    "@types/node": "^24.0.14",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "npm-run-all2": "^8.0.4",
    "oclif": "^4.20.8",
    "ora": "8.2.0",
    "source-map-support": "^0.5.21",
    "ts-node": "10.9.2",
    "tsx": "^4.20.3",
    "typescript": "5.8.3",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.4"
  },
  "bin": {
    "notioncodes": "dist/bin/run.js"
  },
  "oclif": {
    "bin": "notioncodes",
    "commands": "./dist/commands",
    "dirname": "notioncodes",
    "topicSeparator": " ",
    "theme": "./settings/cli-theme.json",
    "plugins": [
      "@oclif/plugin-version",
      "@oclif/plugin-help"
    ]
  },
  "dependencies": {
    "@notion.codes/sdk": "workspace:*",
    "@notion.codes/types": "workspace:*",
    "@inquirer/prompts": "7.6.0",
    "@oclif/plugin-version": "2.2.31",
    "ansis": "4.1.0",
    "fast-levenshtein": "3.0.0",
    "immer": "10.1.1",
    "rxjs": "7.8.2",
    "yaml": "^2.8.0",
    "zod": "^4.0.5",
    "zod-config": "^1.2.0"
  },
  "directories": {
    "doc": "docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mateothegreat/notionkit.git"
  },
  "bugs": {
    "url": "https://github.com/mateothegreat/notionkit/issues"
  },
  "homepage": "https://github.com/mateothegreat/notionkit#readme"
}
